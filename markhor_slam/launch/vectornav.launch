<?xml version="1.0"?>
<launch>
    <arg name="frame_id" default="vectornav" />

    <node pkg="vectornav" type="vnpub" name="vectornav" output="screen">
        <param name="frame_id" value="$(arg frame_id)" />
        <rosparam command="load" file="$(find markhor_slam)/config/vn300.yaml" />
    </node>

</launch>